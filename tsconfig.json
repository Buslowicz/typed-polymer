{
  "compilerOptions": {
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "removeComments": false,
    "sourceMap": false,
    "target": "es5",
    "outDir": "dist",
    "lib": [
      "dom",
      "es6"
    ],
    "declaration": true,
    "declarationDir": "types"
  },
  "exclude": [
    "dist",
    "dist-test",
    "node_modules",
    "types"
  ]
}



